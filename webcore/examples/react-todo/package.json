{
  "name": "react-todo",
  "version": "0.1.0",
  "description": "A TODO list for Dicoogle",
  "main": "module.js",
  "files": [
    "module.js"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-debug": "webpack --config webpack.dev.js",
    "build-watch": "webpack --config webpack.dev.js --watch",
    "prepare": "npm run build"
  },
  "license": "MIT",
  "keywords": [
    "dicoogle",
    "dicoogle-plugin"
  ],
  "dicoogle": {
    "caption": "TODO list",
    "slot-id": "menu",
    "module-file": "module.js"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "file-loader": "^6.2.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
