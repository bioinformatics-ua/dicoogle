    "react-load-image": "^0.1.7",
    "react-router": "^3.2.5",
    "react-router-bootstrap": "^0.20.1",
    "react-select": "^v1.2.0",
    "react-transition-group": "^2.9.0",
    "reflux": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "1.12.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.5",
    "sass-loader": "^10.0.5",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "dev": "DICOOGLE_BASE_URL=http://localhost:8080 webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "precommit": "lint-staged",
    "build-debug": "webpack --config webpack.dev.js --progress",
    "clean": "rimraf dist/* index.html",
    "webpack": "webpack --config webpack.prod.js --progress"
  },
  "lint-staged": {
    "js/*.{js,jsx}": [
      "eslint --fix js",
      "prettier --write js/**/*.{js,jsx}",
      "git add"
    ],
    "*.{js}": [
      "prettier --write *.{js}",
      "git add"
    ],
    "sass/*.scss": [
      "prettier --write sass/**/*.scss",
      "git add"
    ]
  }
