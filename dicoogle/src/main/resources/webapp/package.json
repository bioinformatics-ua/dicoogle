    "Luís A. Bastião <bastiao@bmd-software.com>",
    "Eduardo Pinho <eduardopinho@bmd-software.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bioinformatics-ua/dicoogle"
  },
  "main": "js/app.js",
  "files": [
    "dist/bundle.min.js",
    "dist/bundle.min.js.map",
    "dist/bundle.min.css",
    "dist/bundle.min.css.map",
    "css/",
    "fonts/",
    "assets/",
    "bootstrap/",
    "lib/",
    "index.html"
