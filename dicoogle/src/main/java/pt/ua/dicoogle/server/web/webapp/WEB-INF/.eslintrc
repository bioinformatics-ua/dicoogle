{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "globals": {
    "process": false
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": 0,
    "quotes": 0,
    "camelcase": 0,
    "curly": 0,
    "new-cap": 0,
    "no-alert": 1,
    "key-spacing": 1,
    "no-unused-vars": [1, {"vars": "all", "args": "none"}],
    "comma-spacing": 1,
    "space-infix-ops": 1,
    "no-trailing-spaces": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": 1,
    "semi-spacing": 1,
    "comma-dangle": 1,
    "no-extra-semi": 1,
    "eol-last": 1,
    "consistent-this": [2, "self"],
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-undef": 2
  }
}
