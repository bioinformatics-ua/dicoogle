{
  "name": "dicoogle-webapp",
  "version": "1.0.0",
  "description": "Dicoogle web application",
  "author": "Universidade de Aveiro, DETI/IEETA, Bioinformatics Group (http://bioinformatics.ua.pt/)",
  "homepage": "http://www.dicoogle.com",
  "license": "GPL-3.0+",
  "keywords": [
    "dicoogle",
    "webapp"
  ],
  "contributors": [
    "Luís A. Bastião <bastiao@ua.pt>",
    "Frederico Silva <fredericosilva@ua.pt>",
    "Eduardo Pinho <eduardopinho@ua.pt>"
  ],
  "maintainers": [
    "Luís A. Bastião <bastiao@ua.pt>",
    "Eduardo Pinho <eduardopinho@ua.pt>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bioinformatics-ua/dicoogle"
  },
  "main": "js/app.js",
  "files": [
    "js/bundle.min.js",
    "css/",
    "fonts/",
    "assets/",
    "bootstrap/",
    "lib/",
    "index.html"
  ],
  "dependencies": {
    "bootstrap": "^3.3.2",
    "react-font-awesome": "https://github.com/tkurki/react-font-awesome/tarball/master"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.0.1",
    "browserify-shim": "~3.8.2",
    "datatables": "1.10.8",
    "envify": "^3.0.0",
    "flux": "^2.0.1",
    "htmlprocessor": "^0.1.10",
    "jest-cli": "~0.1.17",
    "jquery": "^1.10.2",
    "jquery-ui": "^1.10.5",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "parallelshell": "^2.0.0",
    "promise": "*",
    "react": "^0.13.3",
    "react-bootstrap": "^0.25.1",
    "react-router": "^0.13.3",
    "react-router-bootstrap": "^0.18.1",
    "reactify": "^1.1.1",
    "reflux": "^0.2.12",
    "rimraf": "latest",
    "uglify-js": "^2.4.24",
    "watchify": "^3.3.1"
  },
  "scripts": {
    "prepublish": "rimraf js/build.js && parallelshell \"npm run css\" \"npm run html\" \"npm run js\"",
    "build": "parallelshell \"npm run html\" \"npm run js\"",
    "js": "NODE_ENV=production browserify . -t babelify | uglifyjs -c dead_code,drop_console,warnings=false -m > js/bundle.min.js",
    "css": "sass sass/dicoogle.scss:css/dicoogle.css",
    "html": "htmlprocessor index-template.html -o index.html -e dist -s",
    "debug": "parallelshell \"npm run js-debug\" \"npm run html-debug\"",
    "js-debug": "browserify . --debug -t babelify > js/bundle.js",
    "html-debug": "htmlprocessor index-template.html -o index.html -e dev -s",
    "css:watch": "sass --watch sass/dicoogle.scss:css/dicoogle.css",
    "js:watch": "watchify -o js/bundle.js -v -d .",
    "test": "jest",
    "clean": "rimraf js/build.js js/build.min.js css/dicoogle.css css/dicoogle.css.map index.html"
  },
  "browserify": {
    "transform": [
      "reactify",
      "babelify",
      "browserify-shim",
      "envify"
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:$"
      ]
    }
  },
  "jest": {
    "rootDir": "./js"
  }
}
