  "main": "js/app.js",
  "dependencies": {
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "jquery": "^1.10.2",
    "jquery-ui":"^1.10.5",
    "bootstrap": "^3.3.2",
    "react": "^0.12.0",
    "react-router": "^0.11.6",
    "react-router-bootstrap": "^0.8.3",
    "react-bootstrap": "^0.13.3",
    "reflux": "^0.2.5",
    "react-font-awesome": "https://github.com/tkurki/react-font-awesome/tarball/master",
    "babel": "4.5.5",
    "datatables": "1.10.5",
    "promise": "*"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "jquery-ui": "./node_modules/jquery-ui/jquery-ui.js",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "devDependencies": {
    "6to5ify": "^4.1.1",
    "babel": "4.5.5",
    "browserify": "^8.1.3",
    "browserify-shim": "~3.8.2",
    "envify": "^3.0.0",
    "jest-cli": "~0.1.17",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d .",
    "test": "jest",
    "build": "NODE_ENV=production ./node_modules/browserify/bin/cmd.js . -t 6to5ify --full-path=false | uglifyjs -cm > js/bundle.min.js",
    "build-debug": "./node_modules/browserify/bin/cmd.js . --debug -t 6to5ify --full-path=false > js/bundle.js"
  },
  "author": "Luís A. Bastião",
  "browserify": {
    "debug": true,
    "transform": [
      "reactify",
      "6to5ify",
      "browserify-shim",
      "envify"

    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:$"
      ]
    }
  },
  "jest": {
    "rootDir": "./js"
  }
}
